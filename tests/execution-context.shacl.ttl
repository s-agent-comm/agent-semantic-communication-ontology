@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix ctx: <https://ns.slashlife.ai/agent/execution-context#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# -- Shape for ctx:ExecutionEnvironment --
ctx:ExecutionEnvironmentShape
    a sh:NodeShape ;
    sh:targetClass ctx:ExecutionEnvironment ;
    sh:property [
        sh:path ctx:envType ;
        sh:minCount 1 ;
        sh:maxCount 1 ;
        sh:datatype xsd:string ;
    ] .
